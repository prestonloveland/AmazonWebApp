@{
    ViewData["Title"] = "Home Page";
}
@model BookListViewModel
<!--index page-->
<body class="bg-dark text-light">


    @foreach (var i in Model.Books)
    {
        <partial name="BookSummary" model="i" />
    }

    <div page-model="@Model.PagingInfo" page-action="Index" page-classes-enabled="true"
         page-class="btn" page-class-normal="btn-outline-light" page-class-selected="btn-primary" 
         page-url-category="@Model.CurrentCategory"
         class="btn-group pull-right m-1"></div>
</body>